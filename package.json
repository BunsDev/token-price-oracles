{
  "name": "token-price-oracles",
  "version": "0.1.1",
  "license": "MIT",
  "files": [
    "build",
    "contracts",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/levelkdev/token-price-oracles.git"
  },
  "dependencies": {
    "bn-chai": "^1.0.1",
    "moment": "^2.22.0",
    "openzeppelin-solidity": "2.0.0",
    "solc": "^0.4.24",
    "truffle-blockchain-utils": "^0.0.3",
    "truffle-hdwallet-provider": "^0.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.4",
    "ganache-cli": "^6.1.6",
    "lk-test-helpers": "^0.1.3",
    "openzeppelin-test-helpers": "0.1.2",
    "snazzy": "^7.1.1",
    "solidity-coverage": "^0.5.5",
    "solidity-docgen": "^0.1.0",
    "standard": "^10.0.3",
    "tidbit": "github:levelkdev/tidbit#6bc85538f01b8781301dbbf8d8602b9db570367b",
    "truffle": "4.1.15",
    "web3": "1.0.0-beta.35"
  },
  "scripts": {
    "cleanup": "rm -rf build/ && mkdir build && rm -rf lib/ && mkdir lib",
    "compile": "npm run cleanup && truffle compile --all && npm run build",
    "build": "node scripts/build.js",
    "build-docs": "scripts/buildDocs.sh",
    "deploy": "npm run cleanup && truffle migrate --reset && npm run build",
    "deploy-rinkeby": "npm run cleanup && truffle migrate --reset --network rinkeby && npm run build",
    "test": "npm run build && scripts/test.sh",
    "test:truffle": "truffle test",
    "test:coverage": "npm run build && npm run solidity-coverage",
    "ganache-cli": "scripts/runGanacheCLI.sh",
    "lint": "standard --verbose | snazzy",
    "tv": "truffle version"
  },
  "description": "tools for creating token price data feeds and oracles that integrate with on-chain dex's",
  "bugs": {
    "url": "https://github.com/levelkdev/token-price-oracles/issues"
  },
  "homepage": "https://github.com/levelkdev/token-price-oracles#readme",
  "keywords": [
    "oracles",
    "ethereum",
    "solidity",
    "tokens",
    "token",
    "prices",
    "datafeed"
  ],
  "author": "levelk"
}
